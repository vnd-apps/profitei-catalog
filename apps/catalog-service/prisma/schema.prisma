datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  createdAt   DateTime @default(now())
  description String?
  float       Decimal?
  id          String   @id @default(cuid())
  kind        String?
  name        String?
  price       Float?
  quality     String?
  updatedAt   DateTime @updatedAt
}
